{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "implicit-arrow-linebreak": "off",
        "no-return-await": "off",
        "no-param-reassign": "off",
        "no-underscore-dangle": "off",
        "indent": "warn",
        "no-unused-vars": "warn",
        "linebreak-style": "off",
        "consistent-return": "off",
        "function-paren-newline": "off",
        "camelcase": "error",
        "import/prefer-default-export": "off",
        "object-curly-newline":"off"
    }
}
